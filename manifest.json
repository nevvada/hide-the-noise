{
  "name": "Hide the Noise",
  "version": "1.0",
  "description": "Hides miscellaneous elements on MDN!",
  "content_scripts": [
    {
      "js": ["content.js"],
      "css": ["mdn_update.css"],
      "matches": [
        "https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/*"
      ]
    }
  ],
  "page_action": {
    "default-icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
